# inflow-ai-core Configuration
# Copy this file to .env and fill in your actual Azure OpenAI values

# Azure OpenAI Configuration (REQUIRED)
AZURE_OPENAI_ENDPOINT=https://your-resource.openai.azure.com/
AZURE_OPENAI_API_KEY=your-api-key-here

# Optional: API Version (defaults to 2024-12-01-preview)
AZURE_OPENAI_API_VERSION=2024-12-01-preview

# Optional: Default model deployment name
# This should match your Azure OpenAI deployment name
AZURE_OPENAI_DEPLOYMENT=gpt-4o
